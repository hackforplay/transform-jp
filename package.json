{
  "name": "transform-jp",
  "version": "0.0.1",
  "description": "Translate javascript to Japanese (Experimental)",
  "main": "index.js",
  "scripts": {
    "kana": "tsc && node lib/kana.js",
    "gen": "tsc && node lib/generate.js",
    "ast": "tsc && node lib/ast.js",
    "con": "tsc && node lib/concat.js",
    "map": "tsc && node lib/map.js",
    "format": "prettier 'src/**/*' --write",
    "test": "tsc && ava"
  },
  "keywords": [
    "babel",
    "Japanese"
  ],
  "author": "teramotodaiki",
  "license": "ISC",
  "dependencies": {
    "@types/babel-generator": "^6.25.2",
    "@types/babel-traverse": "^6.25.4",
    "@types/babel-types": "^7.0.4",
    "@types/babylon": "^6.16.3",
    "@types/node": "^10.3.5",
    "babel-generator": "^6.26.1",
    "babel-standalone": "^6.26.0",
    "babel-traverse": "^6.26.0",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0"
  },
  "devDependencies": {
    "@types/mkdirp": "^0.5.2",
    "ava": "1.0.0-beta.6",
    "mkdirp": "^0.5.1",
    "prettier": "^1.13.5",
    "ts-node": "^6.1.1",
    "typescript": "^2.9.2"
  }
}
